<legend>Player Info</legend>
<dl class="dl-horizontal">
  <dt>Team</dt>
  <dd>{{team.name}}</dd>

  <dt>Player Name</dt>
  <dd>{{fullName}}</dd>

  <dt>Number</dt>
  <dd>{{number}}</dd>

  <dt>Position</dt>
  <dd>{{position}}</dd>

  <dt>Height</dt>
  <dd>{{height}}</dd>

  <dt>Weight</dt>
  <dd>{{weight}}</dd>

  <dt>Date of Birth</dt>
  <dd>{{birthDate}}</dd>


</dl>